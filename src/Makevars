include $(BIOINFOCONFDIR)/prpidefs.make

## No R specific settings are needed, they are automatically handled by R CMD SHLIB

## BIOS specific

CFLAGSL= $(CFLAGS) $(defines)

PKG_CFLAGS= $(CFLAGSL) $(PRPIINC)

PKG_LIBS=-fPIC -I$(BIOINFOCONFDIR) $(PRPISRC)/log.c $(PRPISRC)/arg.c $(PRPISRC)/format.c $(PRPISRC)/array.c $(PRPISRC)/hlrmisc.c $(PRPISRC)/sequtil.c $(PRPISRC)/linestream.c $(PRPISRC)/statistics.c  $(PRPISRC)/recipes.c $(PRPISRC)/matvec.c $(PRPISRC)/combi.c

